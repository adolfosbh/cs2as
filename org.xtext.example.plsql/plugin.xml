<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
	<extension point="org.eclipse.emf.ecore.generated_package">
		<package 
			uri = "http://www.xtext.org/example/plsql/DDL"
			class = "org.xtext.example.plsql.dDL.DDLPackage"
			genModel = "model/generated/DDL.genmodel" />
	</extension>
   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated astm -->
      <package
            uri="http://modelum.es/modernization/astm"
            class="astm.AstmPackage"
            genModel="model/astm.genmodel"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated Lookup -->
      <package
            uri="http://xtext.eclipse.org/example/plsql/lookup"
            class="org.xtext.example.plsql.astm.lookup.LookupPackage"
            genModel="model/Lookup.genmodel"/>
   </extension>

</plugin>
